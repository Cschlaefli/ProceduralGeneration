func _ready():
	set_process_input(true)
	

func _input(event):
	
	if event.is_action(\"ui_up\") :
		position.y -= 10
	if event.is_action(\"ui_down\") :
		position.y += 10
	if event.is_action(\"ui_left\") :
		position.x -= 10
	if event.is_action(\"ui_right\") :
		position.x += 10
	
	if event.is_action_pressed(\"zoom_out\"):
		zoom += Vector2(.1,.1)
	if event.is_action_pressed(\"zoom_in\"):
		zoom -= Vector2(.1,.1)
	
	
"

